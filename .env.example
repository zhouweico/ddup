# 服务器配置
SERVER_PORT=8080
SERVER_MODE=development

# 数据库配置
DB_HOST=postgres
DB_PORT=5432
DB_NAME=ddup
DB_USER=ddup
DB_PASSWORD=Ddup@123456
DB_MAX_OPEN_CONNS=10
DB_MAX_IDLE_CONNS=5
DB_MAX_LIFETIME=1h
DB_MAX_IDLE_TIME=30m
DB_RETRY_TIMES=3
DB_RETRY_DELAY=5s

# JWT配置
JWT_SECRET=your-secret-key 
JWT_EXPIRES_IN=86400

# 健康检查配置
HEALTH_CHECK_INTERVAL=5m

# 日志配置
LOG_LEVEL=info
LOG_FILENAME=app.log
LOG_MAX_SIZE=100
LOG_MAX_BACKUPS=5
LOG_MAX_AGE=30
LOG_COMPRESS=true

# Swagger 配置
SWAGGER_HOST=localhost:8080
SWAGGER_SCHEMES=http

