# 复制此文件到 .env 并填写实际值
APP_PORT=8080
APP_ENV=development

DB_HOST=localhost
DB_PORT=5432
DB_NAME=ddup
DB_USER=ddup
DB_PASSWORD=Ddup@123456

# 数据库连接池配置
