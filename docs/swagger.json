        "dto.CreateProfileRequest": {
            "type": "object",
            "required": [
                "title",
                "type"
            ],
            "properties": {
                "description": {
                    "type": "string",
                    "example": "这是一段描述"
                },
                "end_date": {
                    "type": "string",
                    "example": "2024-06-30T00:00:00Z"
                },
                "location": {
                    "type": "string",
                    "example": "北京"
                },
                "metadata": {
                    "type": "string",
                    "example": "{\"degree\":\"学士\"}"
                },
                "organization": {
                    "type": "string",
                    "example": "测试大学"
                },
                "start_date": {
                    "type": "string",
                    "example": "2020-09-01T00:00:00Z"
                },
                "title": {
                    "type": "string",
                    "example": "测试大学"
                },
                "type": {
                    "type": "string",
                    "example": "education"
                },
                "url": {
                    "type": "string",
                    "example": "https://example.com"
                },
                "visibility": {
                    "type": "string",
                    "example": "public"
                },
                "year": {
                    "type": "integer",
                    "example": 2020
                }
            }
        },
